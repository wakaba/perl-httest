<script type="text/javascript">
Event.observe( window, 'load', function(){
        var target_form = document.forms['edit'].cname;
        var init_value = target_form.value;
        Event.observe( target_form, 'change', function(){
                if ( ( init_value == 'deleted' && target_form.value != 'deleted' )
                     || ( init_value != 'deleted' && target_form.value == 'deleted' ) ) {
                    Element.show('category-change-comment');
                    Element.show('category-change-comment-message');
                }
                else {
                    Element.hide('category-change-comment');
                    Element.hide('category-change-comment-message');
                }
            });
    });
</script>

